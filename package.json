{
  "name": "sandbox",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm dev:client\" \"pnpm dev:worker\"",
    "dev:client": "vite",
    "dev:worker": "wrangler dev --config worker/wrangler.toml --port 8787",
    "build": "vite build",
    "build:worker": "wrangler deploy --config worker/wrangler.toml --dry-run",
    "deploy": "pnpm run deploy:client && pnpm run deploy:worker",
    "deploy:client": "pnpm run build && wrangler pages deploy dist/client --project-name=sandbox-page",
    "deploy:worker": "wrangler deploy --config worker/wrangler.toml",
    "lint": "eslint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251217.0",
    "@hono/node-server": "^1.19.7",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.0",
    "@types/three": "^0.181.0",
    "@uiw/react-textarea-code-editor": "^3.1.1",
    "fengari-web": "^0.1.4",
    "hono": "^4.11.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router-dom": "^7.11.0",
    "three": "^0.181.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.13",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^7.3.0",
    "vitest": "^4.0.13",
    "wrangler": "^4.55.0"
  }
}
